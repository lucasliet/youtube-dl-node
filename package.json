{
  "name": "youtube-dl-node",
  "version": "1.0.0",
  "description": "use youtube-dl to download files and host with node",
  "main": "server.js",
  "author": "Lucas Souza <lucasouliveira@gmail.com>",
  "license": "MIT",
  "scripts": {
    "devdb": "ts-node-dev ./node_modules/typeorm/cli.js schema:sync",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "predeploy": "tsc -p .; cp src/package.json ormconfig.js Procfile ./dist",
    "deploy": "gh-pages -d ./dist -b production; rm -rf ./dist",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.10",
    "gh-pages": "^3.1.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  }
}
